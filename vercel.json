{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      },
      {
        "src": "Frontend/**",
        "use": "@vercel/static"
      },
      {
        "src": "Backend/assistant.py",
        "use": "@vercel/python"
      },
      {
        "src": "Backend/helpers.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "main.py"
      },
      {
        "src": "/(.*)",
        "dest": "Frontend/$1"
      },
      {
        "src": "/Bot3.png",
        "dest": "Frontend/Bot3.png"
      }
    ]
  }